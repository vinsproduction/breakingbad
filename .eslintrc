{
    "parser": "babel-eslint",
    "extends": [
        "plugin:react/recommended"
    ],
    "plugins": [
        "react",
        "react-hooks",
        "import"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": 0,
        "react/prop-types": 0
    },
}
